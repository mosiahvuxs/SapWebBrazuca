	<queryString>
		<![CDATA[SELECT
       H.ID, H.DATA_DOCUMENTO, H.DATA_LANCAMENTO, H.PEDIDO_VENDA_ID, Hl.VALOR,
       ITEM_ID, QUANTIDADE, VALOR_UNITARIO, CODIGO_IMPOSTO_ID, CODIGO_BARRAS, PEDIDO_VENDA_LINHA_NUMERO, UPPER(H.CLIENTE_NOME) CLIENTE_NOME,
HL.DESCRICAO, H.DATA_EXPORTACAO

FROM   PUBLIC.HISTORICO_NOTAFISCALSAIDA H, PUBLIC.HISTORICO_NOTAFISCALSAIDA_LINHAS HL

WHERE
       HL.HISTORICO_NOTAFISCALSAIDA_ID = H.ID
       AND CAST(H.DATA_EXPORTACAO AS DATE) >= TO_DATE('25/09/2014', 'DD/MM/YYYY')

ORDER BY H.DATA_EXPORTACAO]]>
